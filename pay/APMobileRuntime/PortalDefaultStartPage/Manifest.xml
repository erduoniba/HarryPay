<?xml version="1.0" encoding="utf-8"?>
<package version="1.0">
	<!--业务包UID -->
	<uid>09999970</uid>

	<!--业务包名称 -->
	<name>startuppage</name>

	<!--业务包介绍 -->
	<descriptor>startuppage</descriptor>

	<!--业务包版本 -->
	<version>1.0.1.1</version>

	<!--业务包运行要求的最低容器版本 -->
    <uses-sdk minSdkVersion="7.0"/>

	<!--业务包使用的能力集 -->
	<uses-feature></uses-feature>

    <!--解释器版本-->
	<app-type>web</app-type>

	<!--业务包运行前提要求 -->
	<requisite></requisite>

	<!--业务包适配描述 -->
	<supports-screens>960*640\480*800\320*480</supports-screens>

	<!--业务包入口图标 -->
	<icon> </icon>

    <!--开始时间 -->
    <starttime>2014-04-11 00:00:00</starttime>

    <!--结束时间 -->
    <endtime>2014-12-31 00:00:00</endtime>

    <!--停留时间，秒 -->
    <showtime>2</showtime>

    <!--渐变时间，秒/10 -->
    <gradualtime>3</gradualtime>


    <!--业务状态 -->
    <status>delay</status>
    
    <!--是否直接显示图片，否则用webapp -->
    <showpicture>true</showpicture>
    
    <!--iphone4上用的图片 -->
    <iphone4picture>background/iphone4.jpg</iphone4picture>
    
    <!--iphone5上用的图片 -->
    <iphone5picture>background/iphone5.jpg</iphone5picture>

    <!--iphone6上用的图片 -->
    <iphone6picture>background/iphone6.jpg</iphone6picture>

    <!--ipad上用的图片 -->
    <ipadpicture>background/ipad.jpg</ipadpicture>

	<!--业务包入口描述文件 -->
	<entry>welcome.html</entry>
</package>